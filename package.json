{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "Express Boilerplate",
  "scripts": {
    "prestart": "npm-run-all --parallel test security-check",
    "start": "npm-run-all --parallel open:src lint:watch",
    "start-seed": "npm-run-all --parallel prestart seedDb open:src lint:watch",
    "seedDb": "babel-node buildScripts/seedDb.js",
    "open:src": "nodemon ./src/app.js  --exec babel-node --presets es2015",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain burt",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "NODE_ENV=test mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test-verbose": "NODE_ENV=development mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js"
  },
  "author": "Marc",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "4.16.2",
    "express-jwt": "^5.3.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "fs": "0.0.1-security",
    "helmet": "^3.9.0",
    "http": "0.0.0",
    "joi": "^13.0.0",
    "json-loader": "^0.5.7",
    "method-override": "^2.3.10",
    "mongoose": "^4.12.1",
    "morgan": "^1.9.0",
    "nsp": "^2.6.2",
    "request": "^2.83.0",
    "whatwg-fetch": "1.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "chalkline": "0.0.5",
    "cheerio": "0.22.0",
    "compression": "^1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "http-status": "^1.0.1",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "jsonify": "0.0.0",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "sinon": "^4.0.1",
    "style-loader": "0.13.1",
    "supertest": "^3.0.0",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-livereload-plugin": "^1.0.0",
    "webpack-md5-hash": "0.0.5"
  }
}
